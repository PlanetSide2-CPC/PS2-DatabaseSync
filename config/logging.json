{
  "version": 1,
  "disable_existing_loggers": false,
  "root": {
    "level": "NOTSET"
  },
  "loggers": {
    "develop": {
      "level": "NOTSET",
      "handlers": [
        "monitor"
      ],
      "propagate": "no"
    },
    "production": {
      "handlers": [
        "monitor"
      ]
    }
  },
  "handlers": {
    "monitor": {
      "class": "logging.StreamHandler",
      "formatter": "default",
      "level": "NOTSET",
      "stream": "ext://sys.stdout"
    },
    "runtime"
  },
  "formatters": {
    "default": {
      "format": "%(asctime)s %(levelname)-8s %(message)s"
    }
  }
}